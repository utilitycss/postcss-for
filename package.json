{
  "name": "@utilitycss/postcss-for",
  "version": "0.0.1",
  "description": "PostCSS plugin that enables SASS-like for loop syntax in your CSS",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "for",
    "loop",
    "sass",
    "from",
    "to",
    "iterate",
    "nested"
  ],
  "author": "Sylvester Aswin Stanley <me@sylvesteraswin.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:utilitycss/postcss-for.git"
  },
  "dependencies": {
    "postcss": "8.2.6",
    "postcss-simple-vars": "6.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "chai": "4.3.0",
    "debug": "^4.3.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.3.0",
    "husky": "^4.0.0",
    "lint-staged": "^10.5.4",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "typescript": "4.1.5"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "mocha"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "prettier --no-config --write",
      "eslint --fix"
    ]
  }
}
